{
  "name": "cdci_project",
  "version": "1.0.0",
  "description": "An Estate Management System that uses CICD for continuous integration and deployment.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cross-env NODE_ENV=development nodemon index.js",
    "start:prod": "cross-env NODE_ENV=production node index.js",
    "start:prod-dev": "cross-env NODE_ENV=production nodemon index.js",
    "client": "npm run dev --prefix client",
    "server": "nodemon server/server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "createadmin": "nodemon ./test/create_admin.js",
    "createproperties": "nodemon ./test/seed_properties.js",
    "createdepartments": "nodemon ./test/createdepartments.js"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-openid-connect": "^2.17.1",
    "install": "^0.13.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.7.0",
    "morgan": "^1.10.0",
    "nodemailer": "^7.0.5",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "zustand": "^5.0.4"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.7"
  },
  "author": "",
  "license": "ISC"
}
